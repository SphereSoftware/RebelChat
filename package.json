{
  "name": "RebelChat",
  "main": "./lib/main",
  "version": "0.8.1",
  "description": "Rebel Chat is a modern way to keep in touch with any team using Slack, Skype, HipChat right from your Atom editor. Keep all your communications at one place",
  "keywords": [
    "rebel",
    "chat",
    "slack",
    "skype",
    "irc",
    "xmpp",
    "hipchat",
    "im",
    "messenger"
  ],
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js lib components"
  },
  "activationCommands": {
    "body": [
      "rebel-chat:toggle"
    ]
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "3.0.0": "getProviders"
      }
    }
  },
  "repository": "https://github.com/SphereSoftware/RebelChat",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "grunt": "^1.0.1",
    "grunt-apm": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0"
  },
  "dependencies": {
    "@slack/client": "^3.8.1",
    "atom-space-pen-views": "^2.2.0",
    "classnames": "^2.2.5",
    "fs-plus": "^2.9.2",
    "fuzzaldrin": "^2.1.0",
    "hipchatter": "^1.0.0",
    "node-xmpp": "^1.0.5",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-file-reader-input": "^1.1.0",
    "react-html-parser": "^1.0.3",
    "react-intl": "^2.2.3",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-actions": "^1.1.0",
    "redux-logger": "^2.7.0",
    "redux-saga": "^0.14.2",
    "skyweb-revised": "git+https://github.com/shemerey/skyweb.git",
    "styled-components": "^1.3.0",
    "twit": "^2.2.5",
    "underscore-plus": "^1.6.6"
  }
}
